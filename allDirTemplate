for i in */ #eagle, plate, naca2412
do
    cd $i

    for j in */ #300, 3000
    do
	cd $j
	
	for k in */ #0, 10, ... , neg45
	do
	    
	    cd $k
	    
	    
	    
	    cd .. #step out of aoa dir

	done
	
	cd .. #step out of Re dir

    done

    cd .. #step out of cross section dir

done #finish main for loop
